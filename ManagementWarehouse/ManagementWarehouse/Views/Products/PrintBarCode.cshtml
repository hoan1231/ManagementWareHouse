
@{
    ViewBag.Title = "PrintBarCode";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="/Content/plugins/Toastrjs/toastr.min.js"></script>
<link rel="stylesheet" href="/Content/plugins/Toastrjs/toastr.min.css" />
<script src="/Content/plugins/jQueryUrl/jquery.url.js" type="text/javascript"></script>
<link href="/crm_manager/CSS/Menu.css" rel="stylesheet" />
<script src="/crm_manager/js/jquery.mptelecom.js"></script>
<script src="~/ware_product/Js/PrintBarCode.js"></script>
<link rel="stylesheet" href="/Content/dist/checkstyle/build.css" />


<div class="nav-tabs-custom">
    <ul class="nav nav-tabs"  style="float:right;">
        <li class="active">

            <a href="#manager" data-toggle="tab">
                <ul class="btn-tasks">
                    <i class="icon fa fa-print"  title="In mã vạch"></i>In mã vạch
                </ul>
            </a>
        </li>
    </ul>
    <div class="tab-content">
        <div class="chart tab-pane active" id="manager">
            <div class="box">
                <div class="box-header with-border">
                    <i class="ion ion-clipboard"></i>
                    <h3 class="box-title">In mã vạch/Nhãn</h3>
                    <div class="col-lg-12">
                      <div class="well well-sm no-print">
                          <div class="form-group">
                              <label for="add_item">Thêm sản phẩm</label>
                              <input type="text" placeholder="Thêm mục" id="txtAdd" class="form-control input-sm" />
                          </div>
                            <form class="bv-form">
                               <div class="controls table-controls">
                                    <table id="tblBarCode" class="table table-bordered table-hover">
                                        <thead>
                                            <tr class="success">
                                                <th>Tên sản phẩm (Mã sản phẩm)</th>
                                                <th>Số lượng</th>
                                                <th>Biến thể</th>
                                                <th class="text-center" style="width:30px;">
                                                    <i class="fa fa-trash-o" style="opacity:0.5; filter:alpha(opacity=50);"></i>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                       </tbody>  
                                    </table>
                                </div>

                                <div class="form-group">
                                    <label for="style">Style *</label>
                                    <select id="ddlStyle" class="form-control input-sm select2 multiline" style="width:100%">
                                        <option value="">-Chọn-</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                       <label class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">In:</label>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Site Name</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Tên sản phẩm</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Giá</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Tiền tệ</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Đơn vị</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Danh mục</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Biến thể</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Ảnh sản phẩm</label>
                                    </span>
                                    <span class="checkbox checkbox-success styleCheckBoxMenu checkbox-inline">
                                        <input id="chkSysEnable1" class="styled" type="checkbox">
                                        <label for="chkSysEnable1">Check Promo</label>
                                    </span>
                                  
                                   
                                </div>
</form>
                                <div class="form-group">
                                    <input type="button" id="btnUpdate" value="Cập nhật" class="btn btn-primary">
                                    <button type="button" id="btnReset" class="btn btn-danger">Làm lại</button>
                                </div>
                           <div class="clearfix"></div>
                        </div>
                      
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



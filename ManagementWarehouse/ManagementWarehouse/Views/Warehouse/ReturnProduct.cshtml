
@{
    ViewBag.Title = "ReturnProduct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="/Content/plugins/jQueryUrl/jquery.url.js" type="text/javascript"></script>
<script src="/Content/plugins/Toastrjs/toastr.min.js"></script>
<link rel="stylesheet" href="/Content/plugins/Toastrjs/toastr.min.css" />
<script src="/Content/plugins/datatables/dataTables.select.min.js"></script>
<script src="/Content/plugins/jQueryEditTable/jquery.editable-1.3.3.js"></script>
<script src="~/warehouse/Js/ReturnProduct.js"></script>
<div class="box box-warning">
    <div class="box-header with-border">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Mặt hàng trả lại</h3>
        <span style="float:right;">
            <a data-toggle="dropdown" class="dropdown-toggle" id="modalAdd" href="#">
                <i class="fa fa-plus-circle" data-placement="left" title="" data-original-title="Thêm mới"></i>Thêm mới
            </a>
        </span>
    </div>
    <div class="box-body">
        <div class="row">
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label>Kho hàng</label>
                    <select id="ddlWarehouse" style="width:100%;" class="form-control select2 input-sm select2-hidden-accessible" tabindex="-1" aria-hidden="true">
                        <option value="">-- Chọn kho hàng --</option>
                        <option value="">Kho hàng 1</option>
                        <option value="">Kho hàng 2</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label>Sản phẩm</label>
                    <select id="ddlProduct" style="width:100%;" class="form-control select2 input-sm select2-hidden-accessible" tabindex="-1" aria-hidden="true">
                        <option value="">-- Chọn sản phẩm --</option>
                        <option value="">Sửa rửa mặt</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label>Từ ngày </label>
                    <input type="text" id="txtFrom" value="05/05/2019" class="form-control input-datepicker input-sm">
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label>Đến ngày</label>
                    <input type="text" id="txtTo" value="05/05/2019" class="form-control input-datepicker input-sm">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <button id="btnSearch" class="btn btn-primary btn-sm" type="button">
                <i class="fa fa-search"></i>&nbsp; Tìm kiếm
            </button>
            <button id="btnExport" type="button" class="btn btn-warning btn-sm">
                <i class="fa fa-file-excel-o">&nbsp;Xuất Excel</i>
            </button>
        </div>
    </div>
    <hr>
    <div class="box box-warning">
        <div class="box-header with-border">
            <i class="ion ion-clipboard"></i>
            <h3 class="box-title">Kết quả tìm kiếm</h3>
        </div>
        <div class="box-body">
            <div style="overflow: auto">
                <table id="tblware" class="table table-bordered table-hover">
                    <thead class="scroll-table">
                        <tr class="success">
                            <th>STT</th>
                            <th>Mã sản phẩm</th>
                            <th>Tên sản phẩm</th>
                            <th>Người chuyển</th>
                            <th>Ngày chuyển giao</th>
                            <th>Người trả</th>
                            <th>Ngày trả</th>
                            <th>Số lượng</th>
                            <th>Ghi chú</th>
                        </tr>
                    </thead>
                    <tbody id="tbdware">
                        <tr>
                            <td>0</td>
                            <td>ADE_001</td>
                            <td>Sửa rửa mặt</td>
                            <td>hoanlv</td>
                            <td>02/04/2019</td>
                            <td>Nguyễn Văn A</td>
                            <td>05/05/2019</td>
                            <td>2</td>
                            <td>Mặt hàng bị hỏng
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
</div>




    